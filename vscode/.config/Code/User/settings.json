{
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": true,
  "explorer.openEditors.visible": 0,
  "terminal.integrated.fontFamily": "DroidSansMono Nerd Font",
  "git.autofetch": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "none",
  "workbench.statusBar.feedback.visible": false,
  "window.newWindowDimensions": "maximized",
  "gitlens.toggleLineBlame": true,
  "workbench.colorTheme": "Gruvbox Material Dark",
  "gruvboxMaterial.darkContrast": "medium",
  "gruvboxMaterial.darkPalette": "mix",
  "gruvboxMaterial.darkWorkbench": "material",
  "gruvboxMaterial.highContrast": true,
  "gruvboxMaterial.italicComments": true,
  "files.associations": {
    ".functions": "shellscript",
    "Vagrantfile": "ruby"
  },
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.bindingModeHints.enable": true,
  "rust-analyzer.lens.debug.enable": false,
  "rust-analyzer.lens.implementations.enable": false,
  "rust-analyzer.lens.run.enable": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "workbench.editor.enablePreview": false,
  "workbench.editor.empty.hint": "hidden",
  "breadcrumbs.enabled": false,
  "editor.stickyScroll.enabled": false,
  "window.customTitleBarVisibility": "auto",
  "chat.commandCenter.enabled": false,
  "rust-analyzer.checkOnSave": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "github.copilot.enable": false,
  "github.copilot.inlineSuggest.enable": false,
  "github.copilot.editor.enableAutoCompletions": false,
  "github.copilot.chat.enable": false,
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "window.menuBarVisibility": "compact"
}